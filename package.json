    "@typescript-eslint/parser": "^7.0.0"
  },
  "imports": {
    "#validations/*": "./Utils/Validations/*",
    "#middlewares/*": "./Utils/Middlewares/*",
    "#constants/*": "./Utils/Constants/*",
    "#utils/*": "./Utils/*",
    "#storage/*": "./Storage/*",
    "#user/*": "./User/*",
    "#project/*": "./Project/*",
    "#controllers/*": "./Controllers/*",
    "#routes/*": "./Routes/*",
